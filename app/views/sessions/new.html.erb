<div class="intro-header2">
  <h1 class="text-center" style="font-weight: normal; color:white;">Log In</h1>
  <hr style="width:30%;"/>
  <div class="row">
    <div class="col-md-4 col-md-offset-4 col-sm-4 col-xs-8 col-xs-offset-2">
      <div class="well" id="about-well">
        <%= form_for(:session, url: login_path) do |f| %>
            <%= f.label :email, style: "color:black;" %>
            <%= f.text_field :email, class: "form-control custom-form-control" %><br/>

            <%= f.label :password, style: "color:black;" %>
            <%= f.password_field :password, class: "form-control custom-form-control" %><br/>

            <%= f.submit "Log In", class: "btn btn-block purple-button" %>
        <% end %>
      </div>
    </div>
  </div>
</div>